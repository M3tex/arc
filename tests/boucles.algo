/* Test des différentes boucles implémentées */
PROGRAMME()
VAR i <- 0, n
DEBUT
    n <- LIRE()     // Attention la bande d'entrée ne doit pas être vide
    
    /* Test de la boucle tant que (<=> while) */
    TQ i <= n FAIRE
        i <- i + 1
    FTQ

    /* Test de la boucle faire tant-que (<=> do while) */
    FAIRE 
        i <- i - 1
    TQ i >= 0

    /* Test de la boucle pour (<=> for i in range python) */
    POUR i DANS 0...n FAIRE
        ECRIRE(i)
    FPOUR
FIN 
